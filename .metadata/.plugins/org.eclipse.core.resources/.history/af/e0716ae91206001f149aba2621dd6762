package progrms;

public class SpecCharAccurance {

	public static void main(String[] args) {
		
		String str = "java";
		
		char ch = 'a';
		int count =1;
		for(int i=0;i<=str.length();i++) {
			
			if(str.charAt(i)==ch) {
				
				count++;
			}
		}
      System.out.println("Character a accurce = a "+ "="+ count);
	}

}
